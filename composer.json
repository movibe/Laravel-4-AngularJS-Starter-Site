{
	"name": "bktz/laravel4-angularjs-starter-site",
	"description": "Laravel 4 AngularJS Starter Site",
	"keywords": ["laravel", "angularjs", "starter"],
	"license": "MIT",
	"authors": [{
		"name": "Benjamin Katznelson",
		"homepage": "http://ca.linkedin.com/in/benikatznelson",
		"role": "Developer"
	}, {
		"name": "Willian Ribeiro Angelo",
		"homepage": "http://github.com/movibe",
		"role": "Developer"
	}],
	"require": {
		"laravel/framework": "4.*",
		"laravelbook/ardent": "dev-master",
		"zizaco/entrust": "dev-master",
		"j20/php-uuid": "dev-master"
	},
	"require-dev": {
		"dollar/generators": "dev-master",
		"mockery/mockery": "dev-master@dev",
		"barryvdh/laravel-ide-helper": "dev-master",
		"barryvdh/laravel-debugbar": "1.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan debugbar:publish"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}